    "chai": "^4.4.1",
    "eslint": "^9.2.0",
    "fastify": "^4.27.0",
    "globals": "^15.2.0",
    "htmlhint": "^1.1.4",
    "jsdoc": "^4.0.3",
    "mocha": "^10.4.0",
    "mocha-reporter-gha": "^1.1.1",
    "nyc": "^15.1.0",
